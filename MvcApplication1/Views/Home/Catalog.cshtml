<div class="page">

    @{
        ViewBag.Title = "Каталог книг. Выбрать жанр.";
    }

    <hgroup class="title">
        <h1>@ViewBag.Title.</h1>
        <h2>@ViewBag.Message</h2>
    </hgroup>


    <section class="contact">
        <ul>
            @foreach (var g in ViewBag.GenresCatalog) {

            <li><a href="">@g.GenreTitle</a></li>
            }
        </ul>
            
    </section>
</div>